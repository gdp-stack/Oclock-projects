#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/touch/bin/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/touch/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/node_modules:/home/student/Oclock/Challenges/S17-DevOps/node_modules:/home/student/Oclock/Challenges/node_modules:/home/student/Oclock/node_modules:/home/student/node_modules:/home/node_modules:/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/touch/bin/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/touch/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/node_modules:/home/student/Oclock/Challenges/S17-DevOps/node_modules:/home/student/Oclock/Challenges/node_modules:/home/student/Oclock/node_modules:/home/student/node_modules:/home/node_modules:/node_modules:/home/student/Oclock/Challenges/S17-DevOps/S17E03-o-kanban-docker/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
